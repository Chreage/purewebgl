
precision mediump float;

uniform float hightLight;
uniform sampler2D sampler;
varying vec2 vUV;
varying vec3 vNormal;

void main(void) {
    //gl_FragColor = texture2D(sampler, vUV);
    float I=max(vNormal.z,0.);
    vec3 color=vec3(1.-hightLight, hightLight+vUV.x*0.0001, 0.);
    gl_FragColor = vec4(I*color,1.);
}